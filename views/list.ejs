<%-include("header")-%>
    <div class="box" id="heading">
        <h1>  <%= Tittle %> </h1> </div>
        <div class="box"><% for(var i=0;i<newitems.length;i++){ %>
          <div class="item">
            <input type="checkbox">
            <p>  <%= newitems[i] %></p>
          </div>  
        
       <% }%>
                      
                   
    <form class="item" action="/" method="post">
        <input type="text" name="todo" placeholder="Enter your new item" autocomplete="off">
        <button type="submit" name="list" value="<%= Tittle %>">+</button>
    </form>
</div>
<%-include("footer")-%>